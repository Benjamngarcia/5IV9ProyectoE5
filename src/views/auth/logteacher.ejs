<%- include ('../partials/_headerLogin')  %>
            <div class="form-center">
                <div class="form-container">
                    <form action="/IniciarProf" method="POST" onsubmit="return validarLogin();">
                        <p class="input-format-contact">
                            <input type="email" placeholder="  Correo" name="correo_prof" id="email" required>
                        </p>
                        <p class="input-format-contact">
                            <input type="password" placeholder="  Password" name="pass_prof" id="password" required>
                        </p>
                        <p class="buttonsubmit">
                            <button type="submit" class="ingresar">
                                Ingresar
                            </button>
                        </p>
                    </form>
                    <p class="needhelp">¿Necesitas ayuda? <a href="/#contact">Contáctanos</a></p>
                </div>
            </div>
            <%if(typeof alert != "undefined"){%>
                <%- include ('../partials/alertas')  %>
            <%}%>
